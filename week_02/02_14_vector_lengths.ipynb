   "source": [
    "import numpy as np\n",
    "\n",
    "a=np.random.randint(0, 10, (2,2))\n",
    "\n",
    "def vector_lengths(a):\n",
    "    q1 = np.square(a)\n",
    "    q2 = q1.sum(axis=1)\n",
    "    return np.sqrt(q2)\n",
    "\n",
    "def main():\n",
    "    vector_lengths(a)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
   ]
  }
