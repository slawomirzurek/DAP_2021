    "import numpy as np\n",
    "\n",
    "def multiplication_table(n):\n",
    "    if n >0:\n",
    "        x = np.arange(n)\n",
    "    return x*x.reshape(n,1)\n",
    "\n",
    "def main():\n",
    "    print(multiplication_table(4))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
