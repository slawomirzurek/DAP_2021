    "class Prepend(object):\n",
    "    def __init__(self, s):\n",
    "        self.s = s\n",
    "\n",
    "    def write(self, text):\n",
    "        self.text = text\n",
    "        print(self.s + self.text)\n",
    "        \n",
    "def main():\n",
    "    p = Prepend(\"+++ \")\n",
    "    p.write(\"Hello\")\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
