    "    \n",
    "    dice1 = [*range(1,7)]\n",
    "    dice2 = dice1.copy()\n",
    "\n",
    "    for i in range(len(dice1)):\n",
    "        for x in range(len(dice2)):\n",
    "            w = (i,x)\n",
    "            if (i + x == 5) and (i !=0) and (x !=0):\n",
    "                print(w)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
