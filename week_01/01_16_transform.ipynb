   "source": [
    "s1 = \"1 5 3\"\n",
    "s2 = \"2 6 -1\"\n",
    "\n",
    "def transform(s1, s2):\n",
    "\n",
    "    s1 = list(map(int, s1.split()))\n",
    "    s2 = list(map(int, s2.split()))\n",
    "    \n",
    "    s = [*list(map(lambda i: i[0]*i[1], list(zip(s1,s2))))]\n",
    "    return(s)\n",
    "  \n",
    "transform(s1, s2)"
   ]
