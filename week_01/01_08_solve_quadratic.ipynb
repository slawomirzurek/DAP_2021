   "source": [
    "import math\n",
    "\n",
    "def solve_quadratic(a, b, c):\n",
    "        \n",
    "    d = b*b - 4*a*c\n",
    "    d = math.sqrt(abs(d))\n",
    "        \n",
    "    x1 = ((-b+d) / (2*a))\n",
    "    x2 = ((-b-d) / (2*a))\n",
    "    x = (x1, x2)\n",
    "    return (x)\n",
    "\n",
    "print(solve_quadratic(1,-3,2))\n",
    "print(solve_quadratic(1,2,1))"
   ]
  }
